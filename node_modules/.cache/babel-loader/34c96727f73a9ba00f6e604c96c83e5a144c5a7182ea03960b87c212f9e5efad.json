\n\r\n    <!-- 导览区域标题 -->\r\n    <div class=\"section-title\">\r\n      <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#e50000\" stroke-width=\"2\">\r\n        <path d=\"M12 4l-8 8h16l-8-8z\"/>\r\n        <path d=\"M12 4v16\"/>\r\n      </svg>\r\n      <span>江姐故里园区导览</span>\r\n    </div>\r\n\r\n    <!-- 地图容器 -->\r\n    <div class=\"map-container\">\r\n      <img \r\n        src=\"data:image/svg+xml;base64,\" \r\n        alt=\"江姐故里地图\"\r\n      />\r\n      <button class=\"enter-guide-btn\">进入导览</button>\r\n    </div>\r\n\r\n    <!-- AR地图入口 -->\r\n    <div class=\"ar-entry\" @click=\"openAR\">\r\n      <svg width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"white\" stroke-width=\"2\">\r\n        <path d=\"M21 16V8a2 2 0 00-2-2H5a2 2 0 00-2 2v8\"/>\r\n        <path d=\"M3 10h18\"/>\r\n        <path d=\"M16 21h-6\"/>\r\n        <circle cx=\"12\" cy=\"12\" r=\"3\"/>\r\n      </svg>\r\n      <span>AR实景导览</span>\r\n    </div>\r\n\r\n    <!-- 推荐内容 -->\r\n    <div class=\"section-title\">\r\n      <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#e50000\" stroke-width=\"2\">\r\n        <path d=\"M19 21l-7-4-7 4V5a2 2 0 012-2h10a2 2 0 012 2v16z\"/>\r\n      </svg>\r\n      <span>推荐</span>\r\n    </div>\r\n\r\n    <div class=\"recommend-grid\">\r\n      <div class=\"card\" v-for=\"item in recommendList\" :key=\"item.id\">\r\n        <div class=\"card-img-placeholder\"></div>\r\n        <div class=\"card-title\">{{ item.title }}</div>\r\n        <div class=\"card-desc\">{{ item.desc }}</div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 全景展示 -->\r\n    <div class=\"section-title\">\r\n      <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#e50000\" stroke-width=\"2\">\r\n        <circle cx=\"12\" cy=\"12\" r=\"10\"/>\r\n        <line x1=\"2\" y1=\"12\" x2=\"22\" y2=\"12\"/>\r\n        <line x1=\"12\" y1=\"2\" x2=\"12\" y2=\"22\"/>\r\n      </svg>\r\n      <span>全景展示</span>\r\n    </div>\r\n\r\n    <div class=\"panorama-list\">\r\n      <div class=\"pano-item\" v-for=\"i in 3\" :key=\"i\">\r\n        <div class=\"pano-thumb-placeholder\"></div>\r\n        <div class=\"pano-name\">全景场景 {{ i }}</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'TravelPage',\r\n  data() {\r\n    return {\r\n      recommendList: [\r\n        { id: 1, title: '江姐故居', desc: '了解革命先烈的生平事迹' },\r\n        { id: 2, title: '红梅广场', desc: '纪念江姐精神的主题广场' },\r\n        { id: 3, title: '教育展馆', desc: '沉浸式红色教育体验' },\r\n        { id: 4, title: '烈士陵园', desc: '缅怀英烈，致敬英雄' }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    openAR() {\r\n      alert('AR实景导览功能暂未开放')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 响应式基础设置 */\r\n:root {\r\n  font-size: calc(16px + (24 - 16) * (100vw - 320px) / (1200 - 320));\r\n  /* 在 320px ～ 1200px 之间动态调整根字体大小 */\r\n}\r\n\r\n@media (min-width: 1200px) {\r\n  :root {\r\n    font-size: 18px;\r\n  }\r\n}\r\n\r\n* {\r\n  box-sizing: border-box;\r\n}\r\n\r\n.travel-page {\r\n  padding: 1rem;\r\n  padding-bottom: calc(4rem + env(safe-area-inset-bottom)); /* 适配 iPhone 底部安全区 */\r\n  background-color: #f8f5f0;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n/* === 头部 === */\r\n.header {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.user-avatar img {\r\n  width: 2.25rem;\r\n  height: 2.25rem;\r\n  border-radius: 50%;\r\n}\r\n\r\n.location {\r\n  font-size: 0.875rem;\r\n  text-align: center;\r\n  color: #333;\r\n}\r\n.location span {\r\n  font-size: 0.75rem;\r\n  color: #666;\r\n}\r\n\r\n.settings svg {\r\n  width: 1.25rem;\r\n  height: 1.25rem;\r\n}\r\n\r\n/* === 搜索框 === */\r\n.search-bar {\r\n  display: flex;\r\n  margin-bottom: 1rem;\r\n}\r\n.search-bar input {\r\n  flex: 1;\r\n  padding: 0.5rem 0.75rem;\r\n  border: 1px solid #ccc;\r\n  border-radius: 1.25rem 0 0 1.25rem;\r\n  font-size: 0.875rem;\r\n}\r\n.search-bar button {\r\n  width: 2.5rem;\r\n  background: #eee;\r\n  border: 1px solid #ccc;\r\n  border-left: none;\r\n  border-radius: 0 1.25rem 1.25rem 0;\r\n  cursor: pointer;\r\n}\r\n\r\n/* === 广告横幅 === */\r\n.banner {\r\n  position: relative;\r\n  margin-bottom: 1.25rem;\r\n  border-radius: 0.5rem;\r\n  overflow: hidden;\r\n}\r\n.banner img {\r\n  width: 100%;\r\n  height: auto;\r\n  display: block;\r\n}\r\n.banner-btn {\r\n  position: absolute;\r\n  bottom: 0.625rem;\r\n  right: 0.625rem;\r\n  background: #e50000;\r\n  color: white;\r\n  border: none;\r\n  padding: 0.25rem 0.625rem;\r\n  border-radius: 0.75rem;\r\n  font-size: 0.75rem;\r\n  z-index: 2;\r\n}\r\n\r\n/* === 区块标题 === */\r\n.section-title {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 0.5rem;\r\n  margin: 1.25rem 0 0.75rem;\r\n  font-weight: bold;\r\n  color: #333;\r\n  font-size: 1.125rem;\r\n}\r\n.section-title svg {\r\n  color: #e50000;\r\n  width: 1.25rem;\r\n  height: 1.25rem;\r\n}\r\n\r\n/* === 地图区域 === */\r\n.map-container {\r\n  position: relative;\r\n  border-radius: 0.5rem;\r\n  overflow: hidden;\r\n  margin-bottom: 1rem;\r\n}\r\n.map-container img {\r\n  width: 100%;\r\n  height: auto;\r\n  display: block;\r\n}\r\n.enter-guide-btn {\r\n  position: absolute;\r\n  bottom: 1rem;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  background: #e50000;\r\n  color: white;\r\n  border: none;\r\n  padding: 0.5rem 1.5rem;\r\n  border-radius: 1.25rem;\r\n  font-weight: bold;\r\n  font-size: 1rem;\r\n  z-index: 3;\r\n}\r\n\r\n/* === AR入口 === */\r\n.ar-entry {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  gap: 0.5rem;\r\n  background: #e50000;\r\n  color: white;\r\n  padding: 0.625rem;\r\n  border-radius: 0.5rem;\r\n  margin-bottom: 1.25rem;\r\n  cursor: pointer;\r\n  font-size: 1rem;\r\n}\r\n.ar-entry svg {\r\n  width: 1.25rem;\r\n  height: 1.25rem;\r\n}\r\n\r\n/* === 推荐卡片 === */\r\n.recommend-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(120px, 1fr));\r\n  gap: 0.75rem;\r\n  margin-bottom: 1.25rem;\r\n}\r\n.card {\r\n  background: white;\r\n  border-radius: 0.5rem;\r\n  padding: 0.625rem;\r\n  text-align: center;\r\n}\r\n.card-img-placeholder {\r\n  width: 100%;\r\n  height: 5rem;\r\n  background: #eee;\r\n  border-radius: 0.375rem;\r\n  margin-bottom: 0.5rem;\r\n}\r\n.card-title {\r\n  font-weight: bold;\r\n  font-size: 0.875rem;\r\n  margin-bottom: 0.25rem;\r\n}\r\n.card-desc {\r\n  font-size: 0.75rem;\r\n  color: #888;\r\n}\r\n\r\n/* === 全景展示 === */\r\n.panorama-list {\r\n  display: flex;\r\n  gap: 0.75rem;\r\n  overflow-x: auto;\r\n  padding-bottom: 0.625rem;\r\n  -webkit-overflow-scrolling: touch;\r\n}\r\n.pano-item {\r\n  flex: 0 0 auto;\r\n  text-align: center;\r\n  min-width: 80px;\r\n  width: 90px;\r\n}\r\n.pano-thumb-placeholder {\r\n  width: 100%;\r\n  height: 90px;\r\n  background: #eee;\r\n  border-radius: 0.5rem;\r\n  margin-bottom: 0.375rem;\r\n}\r\n.pano-name {\r\n  font-size: 0.8125rem;\r\n  color: #333;\r\n}\r\n\r\n/* 小屏优化 */\r\n@media (max-width: 375px) {\r\n  .travel-page {\r\n    padding: 0.75rem;\r\n  }\r\n  .enter-guide-btn {\r\n    padding: 0.4rem 1.2rem;\r\n    font-size: 0.9rem;\r\n  }\r\n}\r\n</style>"],"mappings":"AAkGA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,CACb;QAAEC,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,MAAM;QAAEC,IAAI,EAAE;MAAc,CAAC,EAC7C;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,MAAM;QAAEC,IAAI,EAAE;MAAc,CAAC,EAC7C;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,MAAM;QAAEC,IAAI,EAAE;MAAY,CAAC,EAC3C;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,MAAM;QAAEC,IAAI,EAAE;MAAY;IAE9C;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACPC,KAAK,CAAC,cAAc;IACtB;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}