    <svg class=\"map-svg\" viewBox=\"0 0 800 1200\" preserveAspectRatio=\"none\">\r\n          <defs>\r\n            <filter id=\"shadow\" x=\"-50%\" y=\"-50%\" width=\"200%\" height=\"200%\">\r\n              <feDropShadow dx=\"0\" dy=\"4\" stdDeviation=\"6\" flood-color=\"#000\" flood-opacity=\"0.3\" />\r\n            </filter>\r\n          </defs>\r\n          <path d=\"M90 300 C200 240 360 260 480 340 C600 420 720 380 680 500\"\r\n                stroke=\"#12A33A\" stroke-width=\"22\" stroke-linecap=\"round\" fill=\"none\" filter=\"url(#shadow)\" />\r\n        </svg>\r\n\r\n        <!-- Ëµ∑ÁÇπ/ÁªàÁÇπ -->\r\n        <div class=\"marker start\" @click.stop=\"selectStart\">\r\n          <img :src=\"startImg\" alt=\"start\" />\r\n          <div class=\"mark-label\">Ëµ∑ÁÇπ</div>\r\n        </div>\r\n        <div class=\"marker end\" @click.stop=\"selectEnd\">\r\n          <img :src=\"endImg\" alt=\"end\" />\r\n        </div>\r\n\r\n        <!-- POIs ÂàóË°®ÔºàÂèØÊâ©Â±ïÔºâ -->\r\n        <div v-for=\"poi in pois\" :key=\"poi.id\" class=\"poi\" :class=\"poi.cls\" :style=\"{left: poi.left, top: poi.top}\" @click.stop=\"selectPoi(poi)\">\r\n          <img :src=\"poi.img\" :alt=\"poi.title\" />\r\n        </div>\r\n\r\n        <!-- Â∫ïÈÉ®Âç°ÁâáÔºàÁªÑ‰ª∂Ôºâ -->\r\n        <PoiBottomCard v-if=\"selectedPoi\" :poi=\"selectedPoi\" @close=\"clearSelection\" @navigate=\"startNavigation\" @detail=\"viewDetail\" @expanded=\"onCardExpanded\" />\r\n\r\n        <!-- ÂØºËà™‰∏≠ÈÅÆÁΩ© -->\r\n        <div v-if=\"navigating\" class=\"nav-overlay\">ÂØºËà™‰∏≠‚Ä¶</div>\r\n\r\n        <!-- ÊÇ¨ÊµÆÁΩóÁõò/ÂÆö‰ΩçÊåâÈíÆ -->\r\n        <button class=\"compass\" @click.stop=\"centerMap\" aria-label=\"‰∏≠ÂøÉÂÆö‰Ωç\">‚úö</button>\r\n      </div>\r\n    </main>\r\n\r\n    <!-- Â∫ïÈÉ®‰ø°ÊÅØÂç° -->\r\n    <section class=\"bottom-card\">\r\n      <div class=\"card-left\">\r\n        <div class=\"distance\">\r\n          <div class=\"num\">14</div>\r\n          <div class=\"unit\">km</div>\r\n        </div>\r\n        <div class=\"info\">\r\n          <div>ÈÄîÁªèÂú∞ÁÇπÔºöÊü¥Êµ∑‰∫ï / 105ÂéÇ</div>\r\n          <div>È¢ÑËÆ°Ë°åÁ®ãÊó∂Èó¥Ôºö2 Â∞èÊó∂ 25 ÂàÜ</div>\r\n          <div>È§êÈ•ÆËÆ°ÂàíÔºö105Âè∑ËÇ•ËÇ†È¶Ü</div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"card-right\">\r\n        <div class=\"transport\">\r\n          <button class=\"tbtn\">üöó</button>\r\n          <button class=\"tbtn active\">üöå</button>\r\n          <button class=\"tbtn\">üö≤</button>\r\n          <button class=\"tbtn\">üö∂</button>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- ÊÇ¨ÊµÆ CTAÔºàÈÄâÂÆöÂπ∂ÂºÄÂßãÂØºËà™Ôºâ -->\r\n    <button class=\"cta\" :class=\"{disabled: !selectedPoi || navigating}\" :disabled=\"!selectedPoi || navigating\" :style=\"ctaStyle\" @click=\"startNavigation\">ÈÄâÂÆöÂπ∂ÂºÄÂßãÂØºËà™</button>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, computed } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport headerBg from '@/assets/images/beautiful/travel-sky.jpg'\r\nimport mapBg from '@/assets/images/beautiful/bg-zigong.jpg'\r\nimport startImg from '@/assets/images/beautiful/lantern.jpg'\r\nimport endImg from '@/assets/images/beautiful/salt-museum.jpg'\r\nimport poi1 from '@/assets/images/beautiful/food.jpg'\r\nimport poi2 from '@/assets/images/beautiful/dinosaur.jpg'\r\nimport PoiBottomCard from '@/components/PoiBottomCard.vue'\r\n\r\nconst router = useRouter()\r\n\r\nconst pois = ref([\r\n  { id: 1, title: 'Êü¥Êµ∑‰∫ï', desc: 'ÂéÜÂè≤ÈÅóÂùÄÔºåÈÄÇÂêàÊãçÁÖß', img: poi1, left: '20%', top: '16%', cls: 'poi-1' },\r\n  { id: 2, title: '105ÂéÇ', desc: 'Â∑•‰∏öÈÅóÂ≠ò‰∏éÁæéÈ£üÊ±áËÅö', img: poi2, left: '60%', top: '26%', cls: 'poi-2' },\r\n])\r\n\r\nconst selectedPoi = ref(null)\r\nconst navigating = ref(false)\r\nconst cardExpanded = ref(false)\r\n\r\nfunction onCardExpanded(val){\r\n  cardExpanded.value = !!val\r\n}\r\n\r\nconst ctaStyle = computed(()=>{\r\n  if(navigating.value) return { bottom: '116px' }\r\n  if(selectedPoi.value){\r\n    return { bottom: cardExpanded.value ? 'calc(50vh + 20px)' : '110px' }\r\n  }\r\n  return { bottom: '86px' }\r\n})\r\n\r\nfunction chooseLocation() {\r\n  // placeholder for selecting location\r\n  console.log('choose location')\r\n}\r\n\r\nfunction centerMap() {\r\n  // placeholder for centering map\r\n  console.log('center map')\r\n}\r\n\r\nfunction selectPoi(poi){\r\n  selectedPoi.value = poi\r\n  cardExpanded.value = false\r\n}\r\n\r\nfunction selectStart(){\r\n  // optionally select start as nav target\r\n  selectedPoi.value = { id: 'start', title: 'Ëµ∑ÁÇπ - Ê±üÂßêÊïÖÈáå', desc: 'Âá∫ÂèëÁÇπ', img: startImg }\r\n  cardExpanded.value = false\r\n}\r\nfunction selectEnd(){\r\n  selectedPoi.value = { id: 'end', title: 'ÁªàÁÇπ', desc: 'ÁõÆÁöÑÂú∞', img: endImg }\r\n  cardExpanded.value = false\r\n}\r\n\r\nfunction clearSelection(){\r\n  selectedPoi.value = null\r\n  cardExpanded.value = false\r\n}\r\n\r\n// popupStyle Â∑≤ÁßªÈô§Ôºà‰ΩøÁî®Â∫ïÈÉ®Âç°ÁâáÊõø‰ª£ÂºπÁ™óÔºâ\r\n\r\nfunction viewDetail(poi){\r\n  // placeholder ÊâìÂºÄËØ¶ÊÉÖÈ°µÊàñÂ±ïÁ§∫Êõ¥Â§ö‰ø°ÊÅØ\r\n  console.log('view detail', poi)\r\n}\r\n\r\nfunction startNavigation(){\r\n  if(!selectedPoi.value){\r\n    alert('ËØ∑ÂÖàÈÄâÊã©ÂØºËà™ÁõÆÊ†á')\r\n    return\r\n  }\r\n  navigating.value = true\r\n  // ÊòæÁ§∫ÂØºËà™‰∏≠Áä∂ÊÄÅ 800ms ÂêéËøõÂÖ•ÂØºËà™È°µÈù¢\r\n  setTimeout(()=>{\r\n    router.push('/beautiful/ar')\r\n  }, 800)\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.beautiful-map{\r\n  display:flex;\r\n  flex-direction:column;\r\n  min-height:100vh;\r\n  background:#f6f2ed;\r\n}\r\n\r\n.header{\r\n  position:relative;\r\n  height:170px;\r\n  overflow:hidden;\r\n}\r\n.header-bg{\r\n  width:100%;\r\n  height:100%;\r\n  object-fit:cover;\r\n  filter:brightness(0.6);\r\n}\r\n.header-overlay{\r\n  position:absolute;\r\n  inset:0;\r\n  display:flex;\r\n  flex-direction:column;\r\n  align-items:center;\r\n  justify-content:center;\r\n  color:#fff;\r\n  text-align:center;\r\n}\r\n.header-overlay h1{font-size:20px;margin:6px 0 12px;font-weight:600}\r\n.location-pill{\r\n  display:inline-flex;\r\n  align-items:center;\r\n  gap:8px;\r\n  background:rgba(255,255,255,0.95);\r\n  color:#333;\r\n  padding:8px 18px;\r\n  border-radius:24px;\r\n  box-shadow:0 6px 14px rgba(0,0,0,0.15);\r\n  cursor:pointer;\r\n}\r\n.location-pill .pin{font-size:16px}\r\n.location-pill .chev{opacity:0.6}\r\n\r\n.map-area{flex:1;position:relative;padding:10px 12px 40px}\r\n.map-canvas{\r\n  position:relative;\r\n  height:calc(100vh - 360px);\r\n  border-radius:8px;\r\n  overflow:hidden;\r\n  background:#eee;\r\n}\r\n.map-bg{position:absolute;inset:0;width:100%;height:100%;object-fit:cover;opacity:0.95}\r\n.map-svg{position:absolute;inset:0;width:100%;height:100%;pointer-events:none}\r\n\r\n.marker{position:absolute;display:flex;flex-direction:column;align-items:center;gap:6px}\r\n.marker img{width:64px;height:48px;object-fit:cover;border-radius:6px;box-shadow:0 6px 14px rgba(0,0,0,0.2)}\r\n.mark-label{font-size:14px;color:#111;font-weight:500}\r\n.marker.start{left:8%;top:20%}\r\n.marker.end{right:10%;top:42%}\r\n\r\n.poi{position:absolute;width:56px;height:36px;border-radius:6px;overflow:hidden;box-shadow:0 8px 18px rgba(0,0,0,0.18);cursor:pointer;transform:translateY(0);transition:transform .18s}\r\n.poi:hover{transform:translateY(-6px)}\r\n.poi img{width:100%;height:100%;object-fit:cover}\r\n.poi-1{left:20%;top:16%}\r\n.poi-2{left:60%;top:26%}\r\n\r\n.compass{position:absolute;left:14px;bottom:86px;width:52px;height:52px;border-radius:50%;background:rgba(0,0,0,0.7);color:#fff;border:none;font-size:20px;box-shadow:0 6px 18px rgba(0,0,0,0.25);display:flex;align-items:center;justify-content:center}\r\n\r\n\r\n/* ÂØºËà™‰∏≠ËíôÂ±Ç */\r\n.nav-overlay{position:absolute;inset:0;display:flex;align-items:center;justify-content:center;background:linear-gradient(180deg,rgba(0,0,0,0.2),rgba(0,0,0,0.35));color:#fff;font-size:18px;z-index:70}\r\n\r\n.bottom-card{\r\n  position:relative;\r\n  margin:16px 18px 24px;\r\n  background:#fff;\r\n  border-radius:12px;\r\n  display:flex;align-items:center;justify-content:space-between;padding:18px;box-shadow:0 12px 30px rgba(0,0,0,0.12);\r\n}\r\n.card-left{display:flex;gap:12px;align-items:flex-start}\r\n.distance{display:flex;align-items:baseline;gap:6px}\r\n.distance .num{font-size:44px;font-weight:700}\r\n.distance .unit{font-size:14px;color:#666}\r\n.info{font-size:12px;color:#333}\r\n.card-right .transport{display:flex;gap:8px}\r\n.tbtn{background:transparent;border:1px solid #eee;padding:10px;border-radius:8px;font-size:16px}\r\n.tbtn.active{background:#fff;color:#d33;border:1px solid rgba(0,0,0,0.06);box-shadow:0 6px 12px rgba(0,0,0,0.06)}\r\n\r\n/* ÊÇ¨ÊµÆ CTA */\r\n.cta{\r\n  position:fixed;left:50%;transform:translateX(-50%);bottom:86px;background:#d8332a;color:#fff;border:none;padding:12px 22px;border-radius:28px;font-size:16px;box-shadow:0 14px 36px rgba(216,51,42,0.28);z-index:60;transition:bottom .22s ease}\r\n.cta.disabled{opacity:0.6;pointer-events:none}\r\n\r\n@media (max-width:420px){\r\n  .map-canvas{height:56vh}\r\n  .header{height:150px}\r\n  .distance .num{font-size:36px}\r\n  .cta{bottom:72px;padding:12px 18px}\r\n}\r\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAe;;EAChBA,KAAK,EAAC;AAAQ;;;EAYhBA,KAAK,EAAC;AAAU;;;;;;;;EAiCKA,KAAK,EAAC;;;;uBA9CnCC,mBAAA,CA+EM,OA/ENC,UA+EM,GA9EJC,mBAAA,CAUS,UAVTC,UAUS,GATPD,mBAAA,CAAyD;IAApDH,KAAK,EAAC,WAAW;IAAEK,GAAG,EAAEC,MAAA,CAAAC,QAAQ;IAAEC,GAAG,EAAC;uCAC3CL,mBAAA,CAOM;IAPDH,KAAK,EAAC;EAAgB,I,0BACzBG,mBAAA,CAAiB,YAAb,UAAQ,qBACZA,mBAAA,CAIM;IAJDH,KAAK,EAAC,eAAe;IAAES,OAAK,EAAEH,MAAA,CAAAI;qCACjCP,mBAAA,CAA2B;IAArBH,KAAK,EAAC;EAAK,GAAC,IAAE,oBACpBG,mBAAA,CAAsC;IAAhCH,KAAK,EAAC;EAAU,GAAC,UAAQ,oBAC/BG,mBAAA,CAA2B;IAArBH,KAAK,EAAC;EAAM,GAAC,GAAC,mB,UAK1BG,mBAAA,CAsCO,QAtCPQ,UAsCO,GArCLR,mBAAA,CAoCM;IApCDH,KAAK,EAAC,YAAY;IAAES,OAAK,EAAEH,MAAA,CAAAM;MAC9BT,mBAAA,CAA6C;IAAxCH,KAAK,EAAC,QAAQ;IAAEK,GAAG,EAAEC,MAAA,CAAAO,KAAK;IAAEL,GAAG,EAAC;uCAErCM,mBAAA,sBAAyB,E,0BACzBX,mBAAA,CAQM;IAR