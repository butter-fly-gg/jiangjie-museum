'ÁªàÁÇπ', desc: 'ÁõÆÁöÑÂú∞', img: endImg }\r\n  cardExpanded.value = false\r\n}\r\n\r\nfunction clearSelection(){\r\n  selectedPoi.value = null\r\n  cardExpanded.value = false\r\n}\r\n\r\n// popupStyle Â∑≤ÁßªÈô§Ôºà‰ΩøÁî®Â∫ïÈÉ®Âç°ÁâáÊõø‰ª£ÂºπÁ™óÔºâ\r\n\r\nfunction viewDetail(poi){\r\n  // placeholder ÊâìÂºÄËØ¶ÊÉÖÈ°µÊàñÂ±ïÁ§∫Êõ¥Â§ö‰ø°ÊÅØ\r\n  console.log('view detail', poi)\r\n}\r\n\r\nfunction startNavigation(){\r\n  if(!selectedPoi.value){\r\n    alert('ËØ∑ÂÖàÈÄâÊã©ÂØºËà™ÁõÆÊ†á')\r\n    return\r\n  }\r\n  navigating.value = true\r\n  // ÊòæÁ§∫ÂØºËà™‰∏≠Áä∂ÊÄÅ 800ms ÂêéËøõÂÖ•ÂØºËà™È°µÈù¢\r\n  setTimeout(()=>{\r\n    router.push('/beautiful/ar')\r\n  }, 800)\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.beautiful-map{\r\n  display:flex;\r\n  flex-direction:column;\r\n  min-height:100vh;\r\n  background:#f6f2ed;\r\n}\r\n\r\n.header{\r\n  position:relative;\r\n  height:170px;\r\n  overflow:hidden;\r\n}\r\n.header-bg{\r\n  width:100%;\r\n  height:100%;\r\n  object-fit:cover;\r\n  filter:brightness(0.6);\r\n}\r\n.header-overlay{\r\n  position:absolute;\r\n  inset:0;\r\n  display:flex;\r\n  flex-direction:column;\r\n  align-items:center;\r\n  justify-content:center;\r\n  color:#fff;\r\n  text-align:center;\r\n}\r\n.header-overlay h1{font-size:20px;margin:6px 0 12px;font-weight:600}\r\n.location-pill{\r\n  display:inline-flex;\r\n  align-items:center;\r\n  gap:8px;\r\n  background:rgba(255,255,255,0.95);\r\n  color:#333;\r\n  padding:8px 18px;\r\n  border-radius:24px;\r\n  box-shadow:0 6px 14px rgba(0,0,0,0.15);\r\n  cursor:pointer;\r\n}\r\n.location-pill .pin{font-size:16px}\r\n.location-pill .chev{opacity:0.6}\r\n\r\n.map-area{flex:1;position:relative;padding:10px 12px 40px}\r\n.map-canvas{\r\n  position:relative;\r\n  height:calc(100vh - 360px);\r\n  border-radius:8px;\r\n  overflow:hidden;\r\n  background:#eee;\r\n}\r\n.map-bg{position:absolute;inset:0;width:100%;height:100%;object-fit:cover;opacity:0.95}\r\n.map-svg{position:absolute;inset:0;width:100%;height:100%;pointer