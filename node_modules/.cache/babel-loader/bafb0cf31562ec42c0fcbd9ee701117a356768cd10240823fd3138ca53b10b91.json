get\n      selectedPoi.value = {\n        id: 'start',\n        title: 'Ëµ∑ÁÇπ - Ê±üÂßêÊïÖÈáå',\n        desc: 'Âá∫ÂèëÁÇπ',\n        img: startImg\n      };\n      cardExpanded.value = false;\n    }\n    function selectEnd() {\n      selectedPoi.value = {\n        id: 'end',\n        title: 'ÁªàÁÇπ',\n        desc: 'ÁõÆÁöÑÂú∞',\n        img: endImg\n      };\n      cardExpanded.value = false;\n    }\n    function clearSelection() {\n      selectedPoi.value = null;\n      cardExpanded.value = false;\n    }\n\n    // popupStyle Â∑≤ÁßªÈô§Ôºà‰ΩøÁî®Â∫ïÈÉ®Âç°ÁâáÊõø‰ª£ÂºπÁ™óÔºâ\n\n    function viewDetail(poi) {\n      // placeholder ÊâìÂºÄËØ¶ÊÉÖÈ°µÊàñÂ±ïÁ§∫Êõ¥Â§ö‰ø°ÊÅØ\n      console.log('view detail', poi);\n    }\n    function startNavigation() {\n      if (!selectedPoi.value) {\n        alert('ËØ∑ÂÖàÈÄâÊã©ÂØºËà™ÁõÆÊ†á');\n        return;\n      }\n      navigating.value = true;\n      // ÊòæÁ§∫ÂØºËà™‰∏≠Áä∂ÊÄÅ 800ms ÂêéËøõÂÖ•ÂØºËà™È°µÈù¢\n      setTimeout(() => {\n        router.push('/beautiful/ar');\n      }, 800);\n    }\n    const __returned__ = {\n      router,\n      pois,\n      selectedPoi,\n      navigating,\n      cardExpanded,\n      onCardExpanded,\n      ctaStyle,\n      chooseLocation,\n      centerMap,\n      selectPoi,\n      selectStart,\n      selectEnd,\n      clearSelection,\n      viewDetail,\n      startNavigation,\n      ref,\n      computed,\n      get useRouter() {\n        return useRouter;\n      },\n      get headerBg() {\n        return headerBg;\n      },\n      get mapBg() {\n        return mapBg;\n      },\n      get startImg() {\n        return startImg;\n      },\n      get endImg() {\n        return endImg;\n      },\n      get poi1() {\n        return poi1;\n      },\n      get poi2() {\n        return poi2;\n      },\n      PoiBottomCard\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","compu