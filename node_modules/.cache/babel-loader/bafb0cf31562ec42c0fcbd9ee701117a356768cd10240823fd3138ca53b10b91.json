     <div class=\"mark-label\">Ëµ∑ÁÇπ</div>\r\n        </div>\r\n        <div class=\"marker end\" @click.stop=\"selectEnd\">\r\n          <img :src=\"endImg\" alt=\"end\" />\r\n        </div>\r\n\r\n        <!-- POIs ÂàóË°®ÔºàÂèØÊâ©Â±ïÔºâ -->\r\n        <div v-for=\"poi in pois\" :key=\"poi.id\" class=\"poi\" :class=\"poi.cls\" :style=\"{left: poi.left, top: poi.top}\" @click.stop=\"selectPoi(poi)\">\r\n          <img :src=\"poi.img\" :alt=\"poi.title\" />\r\n        </div>\r\n\r\n        <!-- Â∫ïÈÉ®Âç°ÁâáÔºàÁªÑ‰ª∂Ôºâ -->\r\n        <PoiBottomCard v-if=\"selectedPoi\" :poi=\"selectedPoi\" @close=\"clearSelection\" @navigate=\"startNavigation\" @detail=\"viewDetail\" @expanded=\"onCardExpanded\" />\r\n\r\n        <!-- ÂØºËà™‰∏≠ÈÅÆÁΩ© -->\r\n        <div v-if=\"navigating\" class=\"nav-overlay\">ÂØºËà™‰∏≠‚Ä¶</div>\r\n\r\n        <!-- ÊÇ¨ÊµÆÁΩóÁõò/ÂÆö‰ΩçÊåâÈíÆ -->\r\n        <button class=\"compass\" @click.stop=\"centerMap\" aria-label=\"‰∏≠ÂøÉÂÆö‰Ωç\">‚úö</button>\r\n      </div>\r\n    </main>\r\n\r\n    <!-- Â∫ïÈÉ®‰ø°ÊÅØÂç° -->\r\n    <section class=\"bottom-card\">\r\n      <div class=\"card-left\">\r\n        <div class=\"distance\">\r\n          <div class=\"num\">14</div>\r\n          <div class=\"unit\">km</div>\r\n        </div>\r\n        <div class=\"info\">\r\n          <div>ÈÄîÁªèÂú∞ÁÇπÔºöÊü¥Êµ∑‰∫ï / 105ÂéÇ</div>\r\n          <div>È¢ÑËÆ°Ë°åÁ®ãÊó∂Èó¥Ôºö2 Â∞èÊó∂ 25 ÂàÜ</div>\r\n          <div>È§êÈ•ÆËÆ°ÂàíÔºö105Âè∑ËÇ•ËÇ†È¶Ü</div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"card-right\">\r\n        <div class=\"transport\">\r\n          <button class=\"tbtn\">üöó</button>\r\n          <button class=\"tbtn active\">üöå</button>\r\n          <button class=\"tbtn\">üö≤</button>\r\n          <button class=\"tbtn\">üö∂</button>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- ÊÇ¨ÊµÆ CTAÔºàÈÄâÂÆöÂπ∂ÂºÄÂßãÂØºËà™Ôºâ -->\r\n    <button class=\"cta\" :class=\"{disabled: !selectedPoi || navigating}\" :disabled=\"!selectedPoi || na