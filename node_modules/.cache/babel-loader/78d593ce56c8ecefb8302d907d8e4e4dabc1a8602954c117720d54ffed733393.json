0%;\r\n  height: 280px;\r\n}\r\n.video-img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n.play-btn {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  width: 80px;\r\n  height: 80px;\r\n  background: rgba(255,255,255,0.9);\r\n  border-radius: 50%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  font-size: 30px;\r\n  color: #e63946;\r\n}\r\n\r\n.title-bar {\r\n  background-color: #a62b28;\r\n  color: white;\r\n  padding: 12px 16px;\r\n  font-size: 16px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.title-content .main-title {\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n}\r\n.title-content .tags {\r\n  font-size: 12px;\r\n  opacity: 0.9;\r\n  margin-top: 4px;\r\n}\r\n.rating {\r\n  color: #ffd700;\r\n}\r\n\r\n.banner {\r\n  background-color: #fff2cc;\r\n  padding: 10px 16px;\r\n  text-align: center;\r\n  font-size: 14px;\r\n  color: #b7791f;\r\n}\r\n.banner .discount {\r\n  color: #e63946;\r\n  font-weight: bold;\r\n}\r\n\r\n.booking-card {\r\n  width: 100%;\r\n  padding: 20px;\r\n  margin: 16px 0;\r\n  background: #fff;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 8px rgba(0,0,0,0.1);\r\n}\r\n.card-header {\r\n  background-color: #e8e0d5;\r\n  padding: 12px 16px;\r\n  font-weight: bold;\r\n  border-bottom: 1px solid #d4c7b8;\r\n  margin: -20px -20px 16px -20px;\r\n}\r\n.sub-header {\r\n  font-size: 12px;\r\n  font-weight: normal;\r\n  margin-left: 8px;\r\n}\r\n.tab-nav {\r\n  display: flex;\r\n  border-bottom: 1px solid #d4c7b8;\r\n}\r\n.tab {\r\n  flex: 1;\r\n  padding: 10px 0;\r\n  text-align: center;\r\n  font-size: 14px;\r\n  color: #666;\r\n}\r\n.tab.active {\r\n  color: #a62b28;\r\n  border-bottom: 2px solid #a62b28;\r\n  font-weight: bold;\r\n}\r\n\r\n.date-select {\r\n  padding: 0;\r\n}\r\n.date-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bo