<div class=\"info-item\">\r\n              <span class=\"label\">同行人数</span>\r\n              <span class=\"value\">3</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"dashed-divider\"></div>\r\n      <div class=\"qrcode-section\">\r\n        <img src=\"../imgs/二维码.png\" alt=\"预约二维码\" class=\"qrcode\">\r\n        <div class=\"qrcode-label\">20241026 - 1 - 1</div>\r\n        <button class=\"refresh-btn\" @click=\"refreshQrcode\">刷新二维码</button>\r\n      </div>\r\n\r\n      <!-- 印章 - 精准定位与票卡底部重叠 -->\r\n      <div class=\"seal\">\r\n        <img src=\"../imgs/欢迎.png\" alt=\"欢迎印章\">\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 底部按钮容器 -->\r\n    <div class=\"footer-buttons\">\r\n      <button class=\"btn cancel-btn\" @click=\"cancelBooking\"><img src=\"../imgs/取消预约.png\" alt=\"取消预约\"></button>\r\n      <button class=\"btn save-btn\"><img src=\"../imgs/保存至本地.png\" alt=\"保存至本地\"></button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nconst refreshQrcode = () => {\r\n  console.log('刷新二维码')\r\n}\r\n\r\nconst cancelBooking = () => {\r\n  console.log('取消预约')\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 基础布局 - 全屏铺满，适配所有设备 */\r\n.booking-page {\r\n  min-height: 100vh;\r\n  width: 100vw;\r\n  margin: 0;\r\n  padding: 0;\r\n  background: #d6e4f0;\r\n  font-family: \"PingFang SC\", \"Microsoft YaHei\", sans-serif;\r\n  position: relative;\r\n  padding-bottom: 120px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n/* 顶部导航 */\r\n.header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 1rem 2rem;\r\n  color: #333;\r\n}\r\n\r\n.title {\r\n  font-size: 18px;\r\n  font-weight: 600;\r\n}\r\n\r\n.close-btn {\r\n  font-size: 24px;\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n  color: #333;\r\n  padding: 0;\r\n  width: 30px;\r\n  height: 30px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n/* 票卡容器 - 核心布局，与印章精准重叠 */\r\n.ticket-container {\r\n  background: rgba(255, 255, 255, 0.6) url('../imgs/1（背景图）.png') no-repeat center/cover;\r\n  border-radius: 12px;\r\n  overflow: hidden;\r\n  margin: 0 auto 0;\r\n  padding: 0;\r\n  max-width: 580px;\r\n  width: calc(100% - 32px);\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n  position: relative;\r\n  z-index: 1;\r\n}\r\n\r\n/* 票卡头部 */\r\n.card-header {\r\n  background: rgba(255, 255, 255, 0.8) url('../imgs/1（背景图）.png') no-repeat center/cover;\r\n  color: #333;\r\n  padding: 24px 16px;\r\n  text-align: left;\r\n}\r\n\r\n.card-title {\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  line-height: 1.4;\r\n}\r\n\r\n/* 基础信息区 */\r\n.basic-info {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 16px;\r\n  flex-wrap: wrap;\r\n  gap: 16px;\r\n}\r\n\r\n.info-item {\r\n  text-align: center;\r\n  flex: 1;\r\n  min-width: 80px;\r\n}\r\n\r\n.code-item {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}\r\n\r\n.label {\r\n  display: block;\r\n  font-size: 12px;\r\n  color: #666;\r\n  margin-bottom: 4px;\r\n}\r\n\r\n.value {\r\n  font-size: 16px;\r\n  font-weight: 500;\r\n}\r\n\r\n.code {\r\n  background: #333;\r\n  color: #fff;\r\n  padding: 4px 8px;\r\n  border-radius: 4px;\r\n  font-family: monospace;\r\n  font-weight: bold;\r\n  font-size: 16px;\r\n}\r\n\r\n/* 红色功能栏 */\r\n.feature-bar {\r\n  background: #a62b28;\r\n  color: #fff;\r\n  padding: 8px 16px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  font-size: 12px;\r\n  flex-wrap: wrap;\r\n  gap: 8px;\r\n}\r\n\r\n/* 参观信息区 */\r\n.visit-info {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 16px;\r\n  margin: 0;\r\n  flex-wrap: wrap;\r\n  gap: 16px;\r\n}\r\n\r\n/* 虚线分隔线 */\r\n.dashed-divider {\r\n  position: relative;\r\n  height: 10px;\r\n  margin: 4px;\r\n  border-top: 1px dashed #999;\r\n}\r\n\r\n.dashed-divider::before {\r\n  content: '';\r\n  position: absolute;\r\n  left: -8px;\r\n  top: -10px;\r\n  width: 20px;\r\n  height: 20px;\r\n  background: #d6e4f0;\r\n  border-radius: 50%;\r\n}\r\n\r\n.dashed-divider::after {\r\n  content: '';\r\n  position: absolute;\r\n  right: -8px;\r\n  top: -10px;\r\n  width: 20px;\r\n  height: 20px;\r\n  background: #d6e4f0;\r\n  border-radius: 50%;\r\n}\r\n\r\n/* 二维码区域 - 不被印章遮挡 */\r\n.qrcode-section {\r\n  background: transparent;\r\n  padding: 20px 16px;\r\n  text-align: center;\r\n  margin: 0;\r\n  position: relative;\r\n  z-index: 2;\r\n}\r\n\r\n.qrcode {\r\n  width: 180px;\r\n  height: 180px;\r\n  margin: 0 auto 12px;\r\n  display: block;\r\n}\r\n\r\n.qrcode-label {\r\n  font-size: 14px;\r\n  color: #666;\r\n  margin-bottom: 12px;\r\n}\r\n\r\n.refresh-btn {\r\n  background: transparent;\r\n  border: 1px solid #070605e3;\r\n  padding: 6px 12px;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  font-size: 14px;\r\n  color: #333;\r\n}\r\n\r\n/* 印章 - 与票卡底部精准重叠 */\r\n.seal {\r\n  position: absolute;\r\n  bottom: -50px; /* 核心：控制重叠高度 */\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  width: 120px;\r\n  height: 120px;\r\n  border-radius: 50%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: transparent;\r\n  box-shadow: none;\r\n  z-index: 0;\r\n}\r\n\r\n.seal img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: contain;\r\n}\r\n\r\n/* 底部按钮 - 固定在最下方，与印章错位不遮挡 */\r\n.footer-buttons {\r\n  position: fixed;\r\n  bottom: 20px;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  display: flex;\r\n  gap: 20px;\r\n  z-index: 10;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.btn {\r\n  width: auto;\r\n  height: auto;\r\n  padding: 0;\r\n  border: none;\r\n  background: transparent !important;\r\n  cursor: pointer;\r\n}\r\n\r\n.cancel-btn img, .save-btn img {\r\n  width: 150px;\r\n  height: 100px;\r\n  object-fit: contain;\r\n  display: block;\r\n}\r\n\r\n/* 移动端适配 - 固定尺寸，避免缩放错乱 */\r\n@media (max-width: 768px) {\r\n  .booking-page {\r\n    padding: 8px;\r\n    padding-bottom: 100px;\r\n  }\r\n  .ticket-container {\r\n    width: calc(100% - 16px);\r\n  }\r\n  .qrcode {\r\n    width: 180px;\r\n    height: 180px;\r\n  }\r\n  .cancel-btn img, .save-btn img {\r\n    width: 120px;\r\n    height: 80px;\r\n  }\r\n  .seal {\r\n    width: 100px;\r\n    height: 100px;\r\n    bottom: -40px;\r\n  }\r\n}\r\n\r\n/* 网页端适配 - 二维码放大，布局不变 */\r\n@media (min-width: 769px) {\r\n  .qrcode {\r\n    width: clamp(180px, 25vw, 350px);\r\n    height: clamp(180px, 25vw, 350px);\r\n  }\r\n  .seal {\r\n    width: clamp(100px, 15vw, 200px);\r\n    height: clamp(100px, 15vw, 200px);\r\n    bottom: clamp(-40px, -5vw, -80px);\r\n  }\r\n  .footer-buttons {\r\n    gap: clamp(20px, 3vw, 40px);\r\n  }\r\n  .cancel-btn img, .save-btn img {\r\n    width: clamp(120px, 20vw, 250px);\r\n    height: clamp(80px, 15vw, 180px);\r\n  }\r\n}\r\n</style>"],"mappings":";;OAmDaA,UAAqB;OAOrBC,UAAoB;OAMgCC,UAAsB;OAC/CC,UAAuB;;EAhExDC,KAAK,EAAC;AAAc;;EAElBA,KAAK,EAAC;AAAQ;;uBAFrBC,mBAAA,CAkEM,OAlENC,UAkEM,GAjEJC,mBAAA,UAAa,EACbC,mBAAA,CAGM,OAHNC,UAGM,G,0BAFJD,mBAAA,CAAiC;IAA3BJ,KAAK,EAAC;EAAO,GAAC,QAAM,qBAC1BI,mBAAA,CAAwE;IAAhEJ,KAAK,EAAC,WAAW;IAAEM,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,IAAA,CAAAC,OAAO,CAACC,IAAI;KAAgB,GAAC,E,GAGjER,mBAAA,cAAiB,EACjBC,mBAAA,CAmDM;IAnDDJ,KAAK,EAAC;EAAkB,IAC3BG,mBAAA,YAAe,E,m5CAwCfC,mBAAA,CAIM;IAJDJ,KAAK,EAAC;EAAgB,I,0BACzBI,mBAAA,CAAsD;IAAjDQ,GAAqB,EAArBhB,UAAqB;IAACiB,GAAG,EAAC,OAAO;IAACb,KAAK,EAAC;wDAC7CI,mBAAA,CAAgD;IAA3CJ,KAAK,EAAC;EAAc,GAAC,kBAAgB,qBAC1CI,mBAAA,CAAiE;IAAzDJ,KAAK,EAAC,aAAa;IAAEM,OAAK,EAAEQ,MAAA,CAAAC;KAAe,OAAK,E,GAG1DZ,mBAAA,sBAAyB,E,0BACzBC,mBAAA,CAEM;IAFDJ,KAAK,EAAC;EAAM,IACfI,mBAAA,CAAqC;IAAhCQ,GAAoB,EAApBf,UAAoB;IAACgB,GAAG,EAAC;4BAIlCV,mBAAA,YAAe,EACfC,mBAAA,CAGM;IAHDJ,KAAK,EAAC;EAAgB,IACzBI,mBAAA,CAAsG;IAA9FJ,KAAK,EAAC,gBAAgB;IAAEM,OAAK,EAAEQ,MAAA,CAAAE;qCAAeZ,mBAAA,CAAuC;IAAlCQ,GAAsB,EAAtBd,UAAsB;IAACe,GAAG,EAAC;4DACtFT,mBAAA,CAA+E;IAAvEJ,KAAK,EAAC;EAAc,IAACI,mBAAA,CAAyC;IAApCQ,GAAuB,EAAvBb,UAAuB;IAACc,GA