iv>\r\n\r\n    <!-- 热门文创 -->\r\n    <div class=\"section\">\r\n      <div class=\"section-title\">\r\n        热门文创\r\n        <span class=\"arrow-right\">→</span>\r\n      </div>\r\n      <div class=\"hot-items\">\r\n        <div class=\"item-card\" v-for=\"(item, index) in hotItems\" :key=\"index\">\r\n          <div class=\"rank\">{{ item.rank }}</div>\r\n          <div class=\"item-header\">\r\n            <span class=\"label\">{{ item.label }}</span>\r\n            <span class=\"price\">¥{{ item.price }}</span>\r\n          </div>\r\n          <img :src=\"item.img\" alt=\"\" class=\"item-img\" />\r\n          <div class=\"item-title\">{{ item.title }}</div>\r\n          <button class=\"add-btn\">+</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 江姐故里系列 -->\r\n    <div class=\"section\">\r\n      <div class=\"section-title\">\r\n        江姐故里系列\r\n        <span class=\"arrow-right\">→</span>\r\n      </div>\r\n      <div class=\"series-items\">\r\n        <div class=\"item-card\" v-for=\"(item, index) in seriesItems\" :key=\"index\">\r\n          <img :src=\"item.img\" alt=\"\" class=\"item-img\" />\r\n          <div class=\"item-title\">{{ item.title }}</div>\r\n          <div class=\"item-price\">¥{{ item.price }}</div>\r\n          <button class=\"add-btn\">+</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref } from 'vue'\r\n\r\n// 数据定义\r\nconst hotItems = ref([\r\n  {\r\n    rank: 'NO.1',\r\n    label: 'AR ILLUSTRATION TEENAGERS’ ARTBOOK',\r\n    title: '《永远的江姐》青少年AR研学绘本',\r\n    price: 29,\r\n    img: 'data:image/svg+xml;base64,'\r\n  },\r\n  {\r\n    rank: 'NO.2',\r\n    label: 'SNOWY STAMP',\r\n    title: '红梅傲雪公交印章',\r\n    price: 10,\r\n    img: 'data:image/svg+xml;base64,'\r\n  },\r\n  {\r\n    rank: 'NO.3',\r\n    label: 'SUPERNOVA WINE',\r\n    title: '故居传承高粱酒',\r\n    price: 399,\r\n    img: 'data:image/svg+xml;base64,'\r\n  }\r\n])\r\n\r\nconst seriesItems = ref([\r\n  {\r\n    title: '【故里国庆非遗漆扇】',\r\n    price: 29,\r\n    img: 'data:image/svg+xml;base64,'\r\n  },\r\n  {\r\n    title: '【经络疏通养生锤】',\r\n    price: 30,\r\n    img: 'data:image/svg+xml;base64,'\r\n  }\r\n])\r\n</script>\r\n\r\n<style scoped>\r\n/* 全局样式 */\r\n.travel-shop-page {\r\n  min-height: 100vh;\r\n  background-color: #f5f0e6; /* 统一背景色 */\r\n  font-family: 'Microsoft Yahei', sans-serif;\r\n  padding: 16px;\r\n  padding-bottom: calc(80px + env(safe-area-inset-bottom));\r\n}\r\n\r\n/* === 头部 === */\r\n.header {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  margin-bottom: 16px;\r\n}\r\n.back-btn {\r\n  width: 36px;\r\n  height: 36px;\r\n  border-radius: 50%;\r\n  background: rgba(255, 255, 255, 0.8);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  cursor: pointer;\r\n}\r\n.title {\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n  color: #333;\r\n  flex: 1;\r\n  text-align: center;\r\n}\r\n.search-box {\r\n  width: 120px;\r\n  background: rgba(255, 255, 255, 0.8);\r\n  border-radius: 20px;\r\n  padding: 4px 8px;\r\n  margin-left: 12px;\r\n}\r\n.search-box input {\r\n  width: 100%;\r\n  border: none;\r\n  outline: none;\r\n  font-size: 14px;\r\n  background: transparent;\r\n}\r\n\r\n/* === 横幅 === */\r\n.banner {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 200px;\r\n  border-radius: 12px;\r\n  overflow: hidden;\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.08);\r\n  margin-bottom: 20px;\r\n}\r\n.banner img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n/* === 区块标题 === */\r\n.section {\r\n  margin-bottom: 20px;\r\n}\r\n.section-title {\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  color: #333;\r\n  margin-bottom: 12px;\r\n  padding-left: 4px;\r\n  border-left: 3px solid #c43e3a;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 8px;\r\n}\r\n.arrow-right {\r\n  font-size: 14px;\r\n  color: #999;\r\n}\r\n\r\n/* === 热门商品 === */\r\n.hot-item