rgin-bottom: 1rem;\r\n  color: #333;\r\n}\r\n\r\n.title {\r\n  font-size: 1.2rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.close-btn {\r\n  font-size: 1.5rem;\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n  color: #333;\r\n  padding: 0;\r\n  width: 30px;\r\n  height: 30px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n/* 完整参观票容器 - 统一背景图+透明度 */\r\n.ticket-container {\r\n  background: rgba(255, 255, 255, 0.6) url('../imgs/1（背景图）.png') no-repeat center/cover;\r\n  border-radius: 12px;\r\n  overflow: hidden;\r\n  margin-bottom: 1.5rem;\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n/* 卡片部分 */\r\n.card {\r\n  background: transparent; /* 透明，继承容器背景 */\r\n}\r\n\r\n.card-header {\r\n  background: rgba(255, 255, 255, 0.8) url('../imgs/1（背景图）.png') no-repeat center/cover;\r\n  color: #333;\r\n  padding: 2rem 1.5rem;\r\n  text-align: left;\r\n}\r\n\r\n.card-title {\r\n  font-size: 1.4rem;\r\n  font-weight: bold;\r\n  line-height: 1.4;\r\n}\r\n\r\n/* 卡片内容区 - 连续整体 */\r\n.card-content {\r\n  background: transparent; /* 透明，继承容器背景 */\r\n}\r\n\r\n/* 基础信息区 - 无背景色，与卡片融为一体 */\r\n.basic-info {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 1rem 1.5rem;\r\n}\r\n\r\n.info-item {\r\n  text-align: center;\r\n}\r\n\r\n/* 核销码特殊样式 */\r\n.code-item {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}\r\n\r\n.label {\r\n  display: block;\r\n  font-size: 0.8rem;\r\n  color: #666;\r\n  margin-bottom: 0.25rem;\r\n}\r\n\r\n.value {\r\n  font-size: 1.1rem;\r\n  font-weight: 500;\r\n}\r\n\r\n.code {\r\n  background: #333;\r\n  color: #fff;\r\n  padding: 0.3rem 0.8rem;\r\n  border-radius: 4px;\r\n  font-family: monospace;\r\n  font-weight: bold;\r\n  font-size: 1.1rem;\r\n}\r\n\r\n/* 红色功能栏 - 仅视觉区分，无分割线 */\r\n.feature-bar {\r\n  background: #e63946;\r\n  color: #fff;\r\n  padding: 0.75rem 1rem;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  font-size: 0.9rem;\r\n}\